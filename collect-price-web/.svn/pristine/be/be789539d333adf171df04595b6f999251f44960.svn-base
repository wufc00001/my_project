<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.chngc.collect.dao.BusiDictionariesPriceSourceMapper" >
  <resultMap id="BaseResultMap" type="com.chngc.collect.entity.BusiDictionariesPriceSource" >
    <id column="id" property="id" jdbcType="BIGINT" />
    <result column="dictionaries_value" property="dictionariesValue" jdbcType="VARCHAR" />
    <result column="edit_time" property="editTime" jdbcType="TIMESTAMP" />
    <result column="edit_user" property="editUser" jdbcType="VARCHAR" />
  </resultMap>

  <insert id="save" parameterType="com.chngc.collect.entity.BusiDictionariesPriceSource" >
    insert into busi_dictionaries_price_source (id, dictionaries_value, edit_time, 
      edit_user)
    values (#{id,jdbcType=BIGINT}, #{dictionariesValue,jdbcType=VARCHAR}, #{editTime,jdbcType=TIMESTAMP}, 
      #{editUser,jdbcType=VARCHAR})
  </insert>
  <update id="saveUpdate" parameterType="com.chngc.collect.entity.BusiDictionariesPriceSource">
    update busi_dictionaries_price_source
    set
    <if test="dictionariesValue != null">
      dictionaries_value = #{dictionariesValue,jdbcType=VARCHAR},
    </if>
    edit_time = now(),
    <if test="editUser != null">
      edit_user = #{editUser,jdbcType=VARCHAR}
    </if>
    where id = #{id,jdbcType=BIGINT}
  </update>

  <select id="getList" resultMap="BaseResultMap" >
    select id, dictionaries_value, edit_time, edit_user
    from busi_dictionaries_price_source
  </select>
</mapper>